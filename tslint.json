{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
      "max-line-length": {
          "options": [80]
      }
    },
    "rules": {
      "no-var-requires": false,
      "interface-name": false,
      "jsdoc-format": false,
      "no-console": false,
      "no-reference": false,
      "semicolon": false,
      "max-line-length": {
        "options": [120]
      }
    },
    "rulesDirectory": []
}